<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.programacion3.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <data android:mimeType="text/plain"/>
            </intent-filter>
        </activity>
        <activity android:name=".LoginSuccessActivity" />
        <activity android:name=".ProgrammaticViewsActivity" />
        <activity android:name=".LifeCycleActivity" />
        <activity android:name=".MarginPaddingActivity" />
        <activity android:name=".LayoutsActivity" />
        <activity android:name=".WidgetsActivity" />
        <activity android:name=".RelativeLayoutActivity" />
        <activity android:name=".FrameLayoutActivity" />
        <activity android:name=".LinearLayoutActivity" />
        <activity android:name=".TableLayoutActivity" />
        <activity android:name=".ImageActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".ListActivity"></activity>
        <activity android:name=".RecyclerActivity" />
        <activity android:name=".NotificationsActivity" />
    </application>

</manifest>